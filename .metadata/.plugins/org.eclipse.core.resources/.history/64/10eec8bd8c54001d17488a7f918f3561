package com.saveMoney.application;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

@Service
public class ApiService {
	
	@Autowired
	SuperMarket sm;

	public void handleRegisterSuperMarket(RegisterSuperMarket rsm) {
		
	}
	
public void writeSuperMarket() {
		
		sm.setUsername(username);
		sm.setPassword(password);
		sm.setShop_name(shop_name); 
		sm.setShop_address(shop_address);
		sm.setShop_longitutde(shop_longitutde); 
		sm.setShop_latitude(shop_latitude);
		
		sm.writeSuperMarket();
	}
}

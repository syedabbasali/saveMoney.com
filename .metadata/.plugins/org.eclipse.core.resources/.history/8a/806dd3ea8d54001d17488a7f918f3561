package com.saveMoney.application;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

@Service
public class ApiService {
	
	@Autowired
	SuperMarket sm;

	public void handleRegisterSuperMarket(RegisterSuperMarket rsm) {
		sm.setUsername(rsm.username);
		sm.setPassword(rsm.password);
		sm.setShop_name(rsm.shop_name); 
		sm.setShop_address(rsm.shop_address);
		sm.setShop_longitutde(rsm.shop_longitutde); 
		sm.setShop_latitude(rsm.shop_latitude);
		
		sm.writeSuperMarket();
	}
	

}
